{
  "name": "test",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "type-check": "tsc --noEmit",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.(ts||tsx)\"",
    "translation:compile": "formatjs compile-folder lang/raw lang/compiled",
    "translation:extract": "formatjs extract '(components||pages||modules||common)/**/*.ts*' --out-file lang/raw/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translation:push": "node ./scripts/translation-push.js",
    "translation:pull": "node ./scripts/translation-pull.js",
    "test": "jest --watch",
    "docs": "docsify serve ./docs",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci",
    "gen:client:core-local": "openapi-generator-cli generate -i http://localhost:9001/swagger.json -g typescript-axios -o lib/core-client && prettier --write lib/core-client",
    "gen:client:payment-local": "openapi-generator-cli generate -i http://localhost:9002/swagger.json -g typescript-axios --additional-properties=enumPropertyNaming=UPPERCASE -o lib/payment-client && prettier --write lib/payment-client",
    "gen:client": "yarn run gen:client:core && yarn run gen:client:payment",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress:headless:local": "cypress run --config baseUrl=http://localhost:3000",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless:local": "start-server-and-test dev http://localhost:3000 cypress:headless:local"
  },
